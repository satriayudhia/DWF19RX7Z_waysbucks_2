           <Col sm={3} className=\"payment-column\">\n                    <Gap height={8} />\n                    <InputTextPayment value=\"Name\" />\n                    <Gap height={20} />\n                    <InputTextPayment value=\"Email\" />\n                    <Gap height={20} />\n                    <InputTextPayment value=\"Phone\" />\n                    <Gap height={20} />\n                    <InputTextPayment value=\"Pos Code\" />\n                    <Gap height={20} />\n                    <InputTextArea value=\"Address\" />\n                    <Gap height={20} />\n                    <Button className=\"btn-payment\" onClick={() => setPayStatus(true)} title=\"Pay\" />\n                </Col>\n            </Row>\n            <Modal\n                size=\"lg\"\n                show={payStatus}\n                onHide={() => setPayStatus(false)}\n                centered>\n                <Modal.Body className=\"text-center\"><p className=\"order-status\">Thank you for ordering in us, please wait to verify your order</p></Modal.Body>\n            </Modal>\n        </Container>\n    )\n}\n\nexport default Cart\n","D:\\Project-React\\DWF19RX7Z_waysbucks-9.Admin-Page\\src\\pages\\MyProfile\\index.jsx",["148","149"],"D:\\Project-React\\DWF19RX7Z_waysbucks-9.Admin-Page\\src\\pages\\AddProduct\\index.jsx",["150","151","152","153","154","155"],"import React, {useState} from 'react'\nimport './AddProduct.scss'\nimport {useHistory} from 'react-router-dom'\nimport axios from 'axios'\nimport Modal from 'react-bootstrap/Modal'\nimport Container from 'react-bootstrap/Container'\nimport Row from 'react-bootstrap/Row'\nimport Col from 'react-bootstrap/Col'\nimport HeaderAdmin from '../../components/molecules/HeaderAdmin'\nimport InputTextProduct from '../../components/atoms/InputTextProduct'\nimport InputNumberProduct from '../../components/atoms/InputNumberProduct'\nimport InputFile from '../../components/atoms/InputFile'\nimport Button from '../../components/ato